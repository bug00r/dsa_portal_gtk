<?xml version="1.0" encoding="utf-8"?>
<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns_1="xmlns" version="1.0" ns_1:fo="http://www.w3.org/1999/XSL/Format" exclude-result-prefixes="ns_1">
  <xsl:param name="breeds"/>
  <xsl:param name="cultures"/>
