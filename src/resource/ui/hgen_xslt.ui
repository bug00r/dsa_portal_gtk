                            <attribute name="height">0</attribute>
                            <attribute name="xalign">0</attribute>
                            <attribute name="visible">0</attribute>
                            <attribute name="single-paragraph-mode">0</attribute>
                            <attribute name="text">0</attribute>
                            <attribute name="width-chars">0</attribute>
                            <attribute name="max-width-chars">0</attribute>
                          </attributes>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
          <packing>
            <property name="resize">False</property>
            <property name="shrink">True</property>
          </packing>
        </child>
        <child>
          <object class="GtkBox">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="orientation">vertical</property>
            <child>
              <placeholder/>
